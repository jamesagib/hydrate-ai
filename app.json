{
  "expo": {
    "name": "Water AI",
    "slug": "hydrate-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F2EFEB"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hydrate.ai",
      "buildNumber": "32",
      "usesAppleSignIn": true,
      "icon": "./assets/icon.png",
      "entitlements": {
        "com.apple.developer.applesignin": [
          "Default",
          "Default"
        ],
        "aps-environment": "development"
      },
      "infoPlist": {
        "NSUserAuthenticationUsageDescription": "Sign in with Apple is used to authenticate your account.",
        "NSUserNotificationUsageDescription": "We'll send you personalized hydration reminders to help you stay healthy and hydrated throughout the day.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.786448900175-t4l077b9dii6m9fp4e716ro93vnj9lqq"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.786448900175-t4l077b9dii6m9fp4e716ro93vnj9lqq"
            ]
          }
        ]
      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#F2EFEB"
      },
      "package": "com.hydrate.ai",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "scheme": "water-ai",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b4f22400-7632-4de3-bcd1-76119d301f5d"
      }
    },
    "owner": "jamesagib",
    "updates": {
      "url": "https://u.expo.dev/b4f22400-7632-4de3-bcd1-76119d301f5d"
    }
  }
}
